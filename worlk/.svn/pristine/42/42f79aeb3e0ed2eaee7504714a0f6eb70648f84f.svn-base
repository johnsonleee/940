	/* ----------------bable pf-list------------------------------- */
	.pf-list thead th,
	.pf-list tbody tr:last-child {
	    border-bottom: 1px solid #d6d6d6; /* non-RGBA fallback */
	    border-bottom: 1px solid rgba(0,0,0,.1);
	}
	.pf-list tbody th,
	.pf-list tbody td {
	    border-bottom: 1px solid #e6e6e6; /* non-RGBA fallback  */
	    border-bottom: 1px solid rgba(0,0,0,.05);
	}
	.pf-list tbody tr:last-child th,
	.pf-list tbody tr:last-child td {
	    border-bottom: 0;
	}
	.pf-list tbody tr:nth-child(odd) td,
	.pf-list tbody tr:nth-child(odd) th {
	    background-color: #eeeeee; /* non-RGBA fallback  */
	    background-color: rgba(0,0,0,.04);
	}
	.pf-list tbody tr:nth-child(even) td,
	.pf-list tbody tr:nth-child(even) th {
	    background-color: #eeeeee; /* non-RGBA fallback  */
	    background-color: rgba(237, 240, 231, 1);
	}
	
	/* ----------------bable breakpoint------------------------------- */
	.pf-table-breakpoint {
		width: 95%;
		margin: 0 auto;
	}
	
	.pf-table-breakpoint th{
		margin-top: 6px !important;
	}
	
	@media ( min-width: 40em ) {
		.pf-table-breakpoint {
			width: 99%;
			margin: 0 auto;
		}
		/* Show the table header rows and set all cells to display: table-cell */
		.pf-table-breakpoint td,
		.pf-table-breakpoint th,
		.pf-table-breakpoint tbody th,
		.pf-table-breakpoint tbody td,
		.pf-table-breakpoint thead td,
		.pf-table-breakpoint thead th{
			display: table-cell;
			margin: 0;
		}
		/* Hide the labels in each cell */
		.pf-table-breakpoint td .ui-table-cell-label,
		.pf-table-breakpoint th .ui-table-cell-label{
			display: none;
		}
	}
