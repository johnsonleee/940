/* ----------------bable hkc-list------------------------------- */
.hkc-list thead th,
.hkc-list tbody tr:last-child {
    border-bottom: 1px solid #d6d6d6; /* non-RGBA fallback */
    border-bottom: 1px solid rgba(0,0,0,.1);
}
.hkc-list tbody th,
.hkc-list tbody td {
    border-bottom: 1px solid #e6e6e6; /* non-RGBA fallback  */
    border-bottom: 1px solid rgba(0,0,0,.05);
}
.hkc-list tbody tr:last-child th,
.hkc-list tbody tr:last-child td {
    border-bottom: 0;
}
.hkc-list tbody tr:nth-child(odd) td,
.hkc-list tbody tr:nth-child(odd) th {
    background-color: #eeeeee; /* non-RGBA fallback  */
    background-color: rgba(0,0,0,.04);
}
.hkc-list tbody tr:nth-child(even) td,
.hkc-list tbody tr:nth-child(even) th {
    background-color: #eeeeee; /* non-RGBA fallback  */
    background-color: rgba(237, 240, 231, 1);
}
/* ----------------bable breakpoint------------------------------- */
.hkc-table-breakpoint {
	width: 95%;
	margin: 0 auto;
}
.hkc-table-breakpoint th {
	margin-top: 6px !important;
}
@media ( min-width: 40em ) {
	.hkc-table-breakpoint {
		width: 99%;
		margin: 0 auto;
	}
	/* Show the table header rows and set all cells to display: table-cell */
	.hkc-table-breakpoint td,
	.hkc-table-breakpoint th,
	.hkc-table-breakpoint tbody th,
	.hkc-table-breakpoint tbody td,
	.hkc-table-breakpoint thead td,
	.hkc-table-breakpoint thead th {
		display: table-cell;
		margin: 0;
	}
	/* Hide the labels in each cell */
	.hkc-table-breakpoint td .ui-table-cell-label,
	.hkc-table-breakpoint th .ui-table-cell-label {
		display: none;
	}
}





