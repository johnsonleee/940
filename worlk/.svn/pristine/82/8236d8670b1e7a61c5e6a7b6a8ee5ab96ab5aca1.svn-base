body{
    margin: 0;
}
.userdetail{
	float: left;
	width: auto;
	height: 34px;
	line-height: 36px;
	font-size: 0.9em;
	text-indent: 6px;
	font-weight: bold;
}
.divmainframe{
    width: 100%;
    float: left;
    height: auto;
    margin: 0;
    padding-bottom: 3px;
    background:#ffffff; /*background:#eaebea;*/
    border: #c5c8c3 1px solid;
    overflow: hidden;
 }
.divtop{
    width:100%;
    height: 36px;
    line-height: 36px;
    float: left;
    margin: 0;
    background: rgba(94, 107, 122, 0.16);
}

.clear{
    clear: both;
    display: none;
}
.menubtn{
    text-align: center;
    border: rgb(46, 185, 175) 1px solid;
    -webkit-border-radius: 4px;
    -moz-border-radius: 4px;
    border-radius: 4px;
    margin-top:3px;
    margin-left: 3px;
  /*   background: #faffe3; */
}
.menubtn:hover{
    border: rgba(103, 123, 110, 0.80) 1px solid;
    background: rgba(219, 255, 221, 0.33);
    cursor: pointer;
}
.btnmenu{
    background:url(../images/menu.gif) no-repeat rgba(235, 255, 221, 0.58);
    background-position: -4px -41px;
    width:28px;
    height:28px;
    margin-right:7px;
    margin-top: 3px;
    -webkit-border-radius: 7px;
    -moz-border-radius: 7px;
    border-radius: 7px;
    border: #8bcf51 1px solid;
    float: right;
}
.btnmenu:hover{
    border: rgba(155, 207, 147, 0.59) 1px solid;
    cursor: pointer;
}
.btn1{
    background:rgba(235, 255, 221, 0.58);
    width:auto;
    height:28px;
    line-height: 28px;
    margin-right:3px;
    margin-top: 3px;
    -webkit-border-radius: 7px;
    -moz-border-radius: 7px;
    border-radius: 7px;
    border: #8bcf51 1px solid;
    float: right;
    font-size: 0.9em;
    font-weight: bold;
    color: #0f771e;
}
.btn1:hover{
    border: rgba(155, 207, 147, 0.59) 1px solid;
    cursor: pointer;
}
.btncash{
    background:url(../images/menu.gif) no-repeat rgba(235, 255, 221, 0.58);
    background-position: -3px -78px;
    width:28px;
    height:28px;
    margin-right:3px;
    margin-top: 3px;
    -webkit-border-radius: 7px;
    -moz-border-radius: 7px;
    border-radius: 7px;
    border: #8bcf51 1px solid;
    float: right;
}
.btncash:hover{
    border: rgba(155, 207, 147, 0.59) 1px solid;
    cursor: pointer;
}
.btnmsg{
    background:url(../images/menu.gif) no-repeat rgba(235, 255, 221, 0.58);
    background-position: -4px -5px;
    width:auto;
    min-width: 28px;
    height:28px;
    line-height: 28px;
    margin-right:4px;
    margin-top: 3px;
    -webkit-border-radius: 7px;
    -moz-border-radius: 7px;
    border-radius: 7px;
    border: #8bcf51 1px solid;
    float: right;
    text-indent: 25px;
    font-size: 0.8em;
    color: #a23a40;
}
.btnmsg:hover{
    border: rgba(155, 207, 147, 0.59) 1px solid;
    cursor: pointer;
}
@media ( min-width :1em ) {
    .btnwidth{
        width: 46%;
        height: 36px;
        font-size: 1.2em;
        float: left;
        line-height: 36px;
    }
}
