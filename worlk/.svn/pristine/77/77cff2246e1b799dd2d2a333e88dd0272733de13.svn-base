/*头部公用*/
.nav-header {
  width: 100%;
  height: 0.65rem;
  line-height: 0.65rem;
  color: #fff;
  border-bottom: 2px solid #19460A;
  background: #00736c;
  background-image: linear-gradient(#00736c, #00736c);
  display: -webkit-box;
  display: -moz-box;
  display: -o-box;
  display: -ms-box;
  /*筛选*/
}
.nav-header a.btn-back {
  height: 0.65rem;
  width: .1rem;
  line-height: 0.65rem;
  padding: 0 .3rem;
  display: block;
}
.nav-header .icon-back {
  width: .45rem;
  height: .45rem;
  vertical-align: middle;
  position: relative;
  display: inline-block;
}
.nav-header .icon-back:before {
  content: '';
  display: block;
  position: absolute;
  left: .07rem;
  top: 0;
  width: .3rem;
  height: .3rem;
  border-bottom: .04rem solid #fff;
  border-left: .04rem solid #fff;
  -webkit-transform: scaleY(0.8) rotateZ(45deg);
  -moz-transform: scaleY(0.8) rotateZ(45deg);
  -ms-transform: scaleY(0.8) rotateZ(45deg);
  -o-transform: scaleY(0.8) rotateZ(45deg);
}
.nav-header .icon-back:after {
  content: '';
  display: block;
  position: absolute;
  top: .2rem;
  left: .03rem;
  height: 0;
  width: .45rem;
}
.nav-header .all-title-contact {
  display: block;
  -webkit-box-flex: 1;
  -moz-box-flex: 1;
  -o-box-flex: 1;
  -ms-flex: 1;
  font-size: .26rem;
  font-weight: 400;
  text-align: center;
  line-height: 0.65rem;
  margin: 0;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}
.nav-header .ui-arrow-down {
  position: relative;
}
.nav-header .ui-arrow-down::before {
  width: .1rem;
  height: .1rem;
  display: inline-block;
  border-bottom: .04rem solid #fff;
  border-left: .04rem solid #fff;
  -webkit-transform: scaleY(0.8) rotateZ(-45deg);
  -o-transform: scaleY(0.8) rotateZ(-45deg);
  -ms-transform: scaleY(0.8) rotateZ(-45deg);
  -moz-transform: scaleY(0.8) rotateZ(-45deg);
  position: absolute;
  top: 0.1rem;
  left: 0.1rem;
  content: '';
}
.nav-header .ui-arrow-up::before {
  -webkit-transform: scaleY(0.8) rotateZ(135deg);
  -moz-transform: scaleY(0.8) rotateZ(135deg);
  -o-transform: scaleY(0.8) rotateZ(135deg);
  -ms-transform: scaleY(0.8) rotateZ(135deg);
  position: absolute;
  top: 0.15rem;
}
.nav-header .ui-btn-more {
  position: relative;
  display: inline-block;
  height: 100%;
  line-height: 1rem;
  text-align: center;
  width: .7rem;
  /*    vertical-align: middle;*/
}
