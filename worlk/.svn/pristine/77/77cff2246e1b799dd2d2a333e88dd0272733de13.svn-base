@charset "utf-8";
body,
p,
input,
h1,
h2,
h3,
h4,
h5,
h6,
ul,
li,
dl,
dt,
dd,
form {
  margin: 0;
  padding: 0;
  list-style: none;
  color: #000;
  font-family: "\5FAE\8F6F\96C5\9ED1", Microsoft YaHei;
  vertical-align: middle;
}
/*公用部分*/
html {
  -ms-touch-action: none;
}
.css {
  -webkit-user-select: none;
}
input[type=radio]::-ms-check,
input[type=checkbox]::-ms-check {
  display: none;
}
i {
  font-style: normal;
}
.css {
  -webkit-touch-callout: none;
}
html {
  /*font-size为 320 / 640 * 100 = 50(px);*/
  font-size: 50px;
}
@media screen and (min-width: 800px) {
  html {
    /*font-size为 320 / 640 * 100 = 50(px);*/
    font-size: 12px;
  }
}
/*clear-before*/
* html .clearfix {
  zoom: 1;
}
/* IE6 */
* + html .clearfix {
  zoom: 1;
}
/* IE7 */
.clearfix:after {
  content: ".";
  display: block;
  height: 0;
  clear: both;
  visibility: hidden;
}
.clearfix:before {
  content: ".";
  display: block;
  height: 0;
  clear: both;
  visibility: hidden;
}
body {
