/*公用弹出菜单*/
.jq_more {
  position: fixed;
  left: 0;
  color: #fff;
  z-index: 2;
  font-size: 1rem;
  top: 0.65rem;
  -webkit-transform: translateY(-285%) translateZ(0);
  -webkit-transition: -webkit-transform 0.4s ease-in 0s;
  -moz-transform: translateY(-285%) translateZ(0);
  -moz-transition: -moz-transform 0.4s ease-in 0s;
  -o-transform: translateY(-285%) translateZ(0);
  -o-transition: -o-transform 0.4s ease-in 0s;
  -ms-transform: translateY(-285%) translateZ(0);
  -ms-transition: -ms-transform 0.4s ease-in 0s;
  width: 100%;
  background: #E0DCD2;
}
.jq_more_open {
  /*公用遮罩*/
}
.jq_more_open .jq_more {
  -webkit-transform: translateY(0) translateZ(0);
  -webkit-transition: -webkit-transform 0.4s cubic-bezier(0.175, 0.885, 0.32, 1.1) 0s;
  -moz-transform: translateY(0) translateZ(0);
  -moz-transition: -moz-transform 0.4s cubic-bezier(0.175, 0.885, 0.32, 1.1) 0s;
  -o-transform: translateY(0) translateZ(0);
  -o-transition: -o-transform 0.4s cubic-bezier(0.175, 0.885, 0.32, 1.1) 0s;
  -ms-transform: translateY(0) translateZ(0);
  -ms-transition: -ms-transform 0.4s cubic-bezier(0.175, 0.885, 0.32, 1.1) 0s;
}
.jq_more_open .jq_mark {
  top: 0.65rem;
  background: #000;
  opacity: .85;
  z-index: 1;
  position: absolute;
  left: 0;
  right: 0;
  bottom: 0;
  display: block;
}
