			sign.append(propertiesLoader.getTrim("vinbet.token.ssc"));
			parameters.put(Constants.TOKEN_SSC, MD5Util.md5Hex(sign.toString()));
		} catch (Exception e) {
			session.setAttribute(com.vinbet.mobile.util.Constants.LATEST_ORDER_TIME_SESSION, 0);
