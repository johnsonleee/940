							|| "GDSYXW".equals(gameId) || "SHSYXW".equals(gameId) || "SDSYXW".equals(gameId) || "JXSYXW".equals(gameId) || "LNSYXW".equals(gameId) || "HBSYXW".equals(gameId)
							|| "JSSYXW".equals(gameId)) {
						if ("SERIAL_2_LZ".equals(bon) || "SERIAL_ZHI_2".equals(bon)) {
							if (types.get(i).indexOf("QIAN") >= 0) {
								btyQ.append(types.get(i).replaceAll("QIAN-", "")).append("#");
							} else {
								btyH.append(types.get(i).replaceAll("HOU-", "")).append("#");
							}
							if (types.get(i).indexOf("HOU") >= 0 && flag) {
								rateAll = new StringBuffer(rateAll.toString().substring(0, rateAll.toString().length() - 1));
								rateAll.append("$^");
								flag = false;
							}
						} else if ("SERIAL_ZHI_3".equals(bon)) {
							if (types.get(i).indexOf("QIAN") >= 0) {
								btyQ.append(types.get(i).replaceAll("QIAN-", "")).append("#");
							} else if (types.get(i).indexOf("HOU") >= 0) {
