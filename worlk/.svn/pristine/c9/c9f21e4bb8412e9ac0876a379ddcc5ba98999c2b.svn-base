	@At("ssc/getRateInfo")
	@Ok("json")
	public JsonResult getRateInfo(@Param("gameId") String gameId, @Param("betOn") String betOn, @Param("handType") String handType, @Param("isRefulsh") String isRefulsh, HttpSession session) {
		JsonResult jsonResult = JsonResult.createSuccess();
		SscUser userInfo = (SscUser) session.getAttribute(com.vinbet.mobile.util.Constants.SSC_USER_SESSION);
		Map<String, String> parameters = new HashMap<String, String>();
		parameters.put("betOn", betOn);
		parameters.put("gameId", gameId);
		parameters.put("isRefulsh", isRefulsh);
		parameters.put("userName", userInfo.getAccoutName());
		parameters.put(Constants.TOKEN_SSC, MD5Util.md5Hex(propertiesLoader.getTrim("vinbet.token.ssc")));
		// if (handType != null && !handType.equals("")) {
		parameters.put("handType", userInfo.getCht());
		// userInfo.setCht(handType);
		// }
		log.debug("getRateInfo start --->betOn:" + betOn + "  handType:" + userInfo.getHandype() + "  gameId:" + gameId + "  isRefulsh:" + isRefulsh + "  userName:" + userInfo.getAccoutName());
		try {
			Map<String, Object> map = new HashMap<String, Object>();
			String url = propertiesLoader.getTrim("vinbet.ssc.url") + "/getRateInfo";
			String rateInfoStr = HttpClientUtil.doGetString(url, parameters, false);
			if (null == rateInfoStr || "".equals(rateInfoStr) || "{}".equals(rateInfoStr)) {
				jsonResult = JsonResult.createFalied("系统异常");
				log.error("getRateInfo 接口返回数据异常");
				return jsonResult;
			}
			Map rateInfo = getRateInfoMap(rateInfoStr, parameters.get("betOn"));
			map.put("gameId", gameId);
			// map.put("hcType", handType);
			map.put("bon", betOn);
			map.put("issue", rateInfo.get("issue"));
			map.put("maxw", rateInfo.get("max_win"));
			map.put("ball", rateInfo.get("ball"));
			map.put("oiss", rateInfo.get("newIssue"));
			map.put("optm", (Integer) rateInfo.get("openTime") / 1000);
			map.put("yk", rateInfo.get("ykMoney"));
			String stopBetFlag = "0";
			if ("0".equals(String.valueOf(rateInfo.get("hcStatus"))) || "10".equals(String.valueOf(rateInfo.get("hcStatus")))) {
				stopBetFlag = "1";
			}
			map.put("stopBetFlag", stopBetFlag);

			if (stopBetFlag.equals("1")) {
				map.put("closeCountdown", 0);
			} else {
				int closeTime = (Integer) rateInfo.get("closeTime") / 1000;
				map.put("closeCountdown", closeTime);
			}

			List<Map<String, Object>> list = new ArrayList<Map<String, Object>>();
			Map rateMap = (Map) rateInfo.get("rateMap");
			if (null != rateMap) {
				Iterator it = rateMap.entrySet().iterator();
				while (it.hasNext()) {
					Map.Entry et = (Map.Entry) it.next();
					Map<String, Object> rmap = new HashMap<String, Object>();
					rmap.put("mKey", et.getKey());
					double rate = ArithUtil.formatDouble_3(Double.parseDouble(et.getValue().toString()));
					rmap.put("mValue", rate);
					list.add(rmap);
				}
			}
			jsonResult.addAll(list);
			jsonResult.setData(map);
		} catch (Exception e) {
			jsonResult = JsonResult.createFalied();
			jsonResult.setM("系统异常，请联系客服！");
			log.error("getRateInfo error:", e);
		}
		return jsonResult;
	}

	private Map<String, Object> getRateInfoMap(String rateInfoStr, String betOn) {
		Map<String, Object> rateInfoMap = new HashMap<String, Object>();
		Map map = JSON.parseObject(rateInfoStr, Map.class);
		Iterator iter = map.entrySet().iterator();
		while (iter.hasNext()) {
			Map.Entry entry = (Map.Entry) iter.next();
			String key = (String) entry.getKey();
			Object val = entry.getValue();
			if (!"upyMap".equals(key)) {
				if ("rateMap".equals(key) && null != val) {
					Map<String, Object> rateMap = new HashMap<String, Object>();
					JSONObject rateJson = (JSONObject) map.get("rateMap");
					Map gameRateMap = JSON.parseObject(JSON.toJSONString(rateJson), Map.class);
					Iterator it = gameRateMap.entrySet().iterator();
					while (it.hasNext()) {
						Map.Entry et = (Map.Entry) it.next();
						String gameStr = (String) et.getKey();
						JSONObject noRate = (JSONObject) et.getValue();
						Map noRateMap = JSON.parseObject(JSON.toJSONString(noRate), Map.class);
						if ("LIANGMIAN".equals(betOn) || "ZHHE".equals(betOn)) {
							Iterator noit = noRateMap.entrySet().iterator();
							while (noit.hasNext()) {
								Map.Entry noet = (Map.Entry) noit.next();
								String noStr = (String) noet.getKey();
								if (null == exclusionMap.get(noStr)) {
									Object rate = noet.getValue();
									rateMap.put(gameStr + "-" + noStr, rate);
								}
							}
						} else if ("BALL_1_5".equals(betOn) || "LONGHU".equals(betOn)) {
							Iterator noit = noRateMap.entrySet().iterator();
							while (noit.hasNext()) {
								Map.Entry noet = (Map.Entry) noit.next();
								String noStr = (String) noet.getKey();
								Object rate = noet.getValue();
								rateMap.put(gameStr + "_" + noStr, rate);
							}
						} else {
							if (betOn.equals(gameStr)) {
								Iterator noit = noRateMap.entrySet().iterator();
								while (noit.hasNext()) {
									Map.Entry noet = (Map.Entry) noit.next();
									String noStr = (String) noet.getKey();
									Object rate = noet.getValue();
									rateMap.put(noStr, rate);
								}
							}
						}
					}
					rateInfoMap.put(key, rateMap);
				} else {
					rateInfoMap.put(key, val);
				}
			}
		}
		return rateInfoMap;
	}

	@At("ssc/ballLong")
	@Ok("json")
	public Object getBallLong(String gameId, HttpServletRequest request, HttpSession session) {
		JsonResult jsonResult = JsonResult.createSuccess();
		String url = new StringBuilder(propertiesLoader.getTrim("vinbet.ssc.url")).append("/getBallLong").toString();
		Map<String, String> parameters = new HashMap<String, String>();
		parameters.put("gameId", gameId);
		parameters.put(Constants.TOKEN_SSC, MD5Util.md5Hex(propertiesLoader.getTrim("vinbet.token.ssc")));
		String response = HttpClientUtil.doGetString(url, parameters, false);
		if (response != null && !response.equals("")) {
			Map<String, String> i18n = (Map<String, String>) request.getAttribute("msg");
			try {
				JSONObject jsonObject = JSON.parseObject(response);
				JSONArray jsonArray = JSON.parseArray(jsonObject.get("ballLongMap").toString());
				List<Map<String, Object>> accResults = new ArrayList<Map<String, Object>>();
				for (Object obj : jsonArray) {
					JSONObject jo = (JSONObject) obj;
					Map<String, Object> result = new HashMap<String, Object>();
					result.put("pn", i18n.get("label." + (gameId.equals("BJC") ? "bjc" : "ssc") + ".play." + jo.getString("play")) + i18n.get("label.ssc.ball." + jo.getString("nextPlay")));
					result.put("num", jo.getString("number") + " 期");
					accResults.add(result);
				}
				jsonResult.addAll(accResults);
			} catch (Exception e) {
				jsonResult = JsonResult.createFalied();
				log.error("调用时时彩手机服务器端[affiche]接口返回数据格式异常,response << " + response, e);
			}
		} else {
			jsonResult = JsonResult.createFalied();
			log.error("调用时时彩手机服务器端[affiche]接口返回数据为空,response << " + response);
		}
		return jsonResult;
	}
}
