	public Object toGameResultSingle(@Param("gid") int gameId, String gname, HttpServletRequest request) {
		Map<String, String> i18n = (Map<String, String>) request.getAttribute("msg");
		HashMap<String, Object> obj = new HashMap<String, Object>();
		if (gname != null && !gname.equals("")) {
			gameId = LotteryType.valueOf(gname).getCode();
		}
		obj.put("gid", gameId);
		obj.put("gname", i18n.get("title.ssc." + LotteryType.valueOf(gameId)));
		obj.put("date", DateUtil.format(new Date(), "yyyy-MM-dd"));// 默认为今天
		return obj;
	}

	/**
	 * 获取所有彩种最近一期的开奖结果
	 * 
	 * @return
	 */
	@At("ssc/getGameResultLatest")
	@Ok("json")
	public JsonResult getGameResultLatest(HttpServletRequest request) {
		JsonResult jsonResult = JsonResult.createSuccess();
		String url = new StringBuilder(propertiesLoader.getTrim("vinbet.ssc.url")).append("/openBall").toString();
		Map<String, String> parameters = new HashMap<String, String>();
		parameters.put(Constants.TOKEN_SSC, MD5Util.md5Hex(propertiesLoader.getTrim("vinbet.token.ssc")));
		String response = HttpClientUtil.doGetString(url, parameters, false);
		// String response =
		// "{'OpenBallMap':{'JSKS':'20140522032_1,3,4','BJC':'441106_01,02,03,04,05,06,07,08,09,10','KLC':'2014081035_05,03,01,02,04,06,07,08','XYNC':'20140522073_19,12,06,04,02,11,01,20','SSC':'20140810052_01,02,03,04,05'}}";
		if (response != null && !response.equals("")) {
			try {
				Map<String, String> i18n = (Map<String, String>) request.getAttribute("msg");
				JSONObject jsonObject = JSON.parseObject(response);
				JSONObject openBall = JSON.parseObject(jsonObject.get("OpenBallMap").toString());
				List<Map<String, String>> gameResults = new ArrayList<Map<String, String>>();
				for (LotteryType sscType : LotteryType.values()) {
					if ("0".equals(AppCache.sscSysConfig.get(sscType.toString().toLowerCase()))) {
						continue;
					}
					String resultContent = openBall.getString(sscType.name());
					if (null != resultContent && !"".equals("resultContent")) {
						Map<String, String> result = new HashMap<String, String>();
						String content[] = resultContent.split("_");
						if (content.length >= 2) {
							result.put("gid", String.valueOf(sscType.getCode()));
							result.put("code", sscType.name());
							result.put("name", i18n.get("title.ssc." + sscType.name()));
							result.put("issue", content[0]);// 期数
							result.put("result", content[1]);// 结果
							// result.put("time",
							// DateUtil.getTime(DateUtil.parseDate(content[2],
							// "yyyy-MM-dd HH:mm:ss")));// 开奖时间
							gameResults.add(result);
						}
					}
				}
				jsonResult.addAll(gameResults);
			} catch (Exception e) {
				jsonResult = JsonResult.createFalied();
				log.error("调用时时彩手机服务器端[openBall]接口返回数据格式异常,response << " + response, e);
			}
		} else {
			jsonResult = JsonResult.createFalied();
			log.error("调用时时彩手机服务器端[openBall]接口返回数据为空");
		}
		return jsonResult;
	}

