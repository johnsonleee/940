	private void setColor(Map<String, Object> result) {
		for (String key : result.keySet()) {
			String tem = result.get(key).toString();
			if (tem.indexOf("大") > -1 || tem.indexOf("双") > -1 || tem.equals("龙")) {
				tem = "<font color='red'>" + tem + "</font>";
			}
			result.put(key, tem);
		}
	}

	private int addUpBall(String ball) {
		int r = 0;
		String[] ba = ball.split(",");
		for (int i = 0; i < ba.length; i++) {
			int a = Integer.parseInt(ba[i].substring(0, 1));
			int b = Integer.parseInt(ba[i].substring(1));
			r += a * 10 + b;
		}
		return r;
	}

