							} else {
								btyS.append(types.get(i).replaceAll("SAN-", "")).append("#");
							}
							if (types.get(i).indexOf("HOU") >= 0 && flag) {
								rateAll = new StringBuffer(rateAll.toString().substring(0, rateAll.toString().length() - 1));
								rateAll.append("$^");
								flag = false;
							}
							if (types.get(i).indexOf("SAN") >= 0 && flags) {
								rateAll = new StringBuffer(rateAll.toString().substring(0, rateAll.toString().length() - 1));
								rateAll.append("$^");
								flags = false;
							}
						} else {
							if ("GDSYXW".equals(gameId) || "SDSYXW".equals(gameId) || "SHSYXW".equals(gameId) || "JXSYXW".equals(gameId) || "LNSYXW".equals(gameId) || "HBSYXW".equals(gameId)
									|| "JSSYXW".equals(gameId)) {
								btyAll.append(types.get(i).replaceAll("-", "_")).append(",");
							} else {
