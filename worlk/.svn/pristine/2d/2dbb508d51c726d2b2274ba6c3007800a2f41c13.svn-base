@media ( min-width :568px ) {
.ico_group{
	width:96px;
	height:100px;
	float:left;
	font-size:12px;
	margin:5px;
	cursor:pointer;
	}
}
@media ( min-width :667px ) {
.ico_group{
	width:94px;
	height:100px;
	float:left;
	font-size:12px;
	margin:5px;
	cursor:pointer;
	}
}

