.ico_frm{
	margin:13px auto;
	background:#ffffff !important;
	width:97%;
	height:auto;
	border:#666 1px dashed;
	-webkit-border-radius: 10px 10px 10px 10px; -moz-border-radius: 10px 10px 10px 10px; border-radius: 10px 10px 10px 10px;
	}
.setting_title{
	width:100%;
	float:left;
	}
.iftitle{
	/* margin:-8px 0 0 12px; */
	margin:-8px 0 20px 12px;
	padding:2px 4px;
	display:inline-table;
	/* background:#ffffff; */
	background:#000;
	color:#fff;
	/* border:#666 1px solid; */
	border:#666 1px dashed;
	font-size:12px;
	letter-spacing:1px;
	float:left;
	padding:0px 5px;
	font-family:"microsoft yahei"; 
	
	}
.ico_group{
	width:100px;
	height:100px;
	float:left;
	font-size:12px;
	margin:6px;
	cursor:pointer;
	}
@media ( min-width :320px ) { /*iphone 4*/
.ico_group{
	width:86px;
	height:100px;
	float:left;
	font-size:12px;
	margin:6px 8px;
	cursor:pointer;
	}
}
@media ( min-width :375px ) { /*iphone 6*/
.ico_frm{
	margin:15px auto;
	background:#ffffff;
	width:90%;
	height:auto;
	border:#666 1px dashed;
	-webkit-border-radius: 10px 10px 10px 10px; -moz-border-radius: 10px 10px 10px 10px; border-radius: 10px 10px 10px 10px;
	}
	
.ico_group{
	width:96px;
	height:100px;
	float:left;
	font-size:12px;
	margin:7px;
	cursor:pointer;
	}
}
@media ( min-width :380px ) { /*mx4*/
.ico_group{
	width:82px;
	height:100px;
	float:left;
	font-size:12px;
	margin:6px 16px;
	cursor:pointer;
	}
}
@media ( min-width :414px ) { /*iphone 6 +*/
.ico_group{
	width:82px;
	height:100px;
	float:left;
	font-size:12px;
