                        </div>
                        <div class="ui-label-text">
                            <div class="ui-label-title">备注</div>
                            <div class="ui-label-content"><a href="#">———</a> </div>
                        </div>
                    </div>
            </div>
        </section>
    </div>
    <!--项目主体end-->
</div>

<div id="top-animation">顶部</div>
<!--公用js-->
<script type="text/javascript" src="../js/jquery-1.9.1.min.js"></script>
<script type="text/javascript" src="../js/comm.js"></script>
<script>
    $(".ui-toggle").click(function(){
        $(this).find(".ui-list-item").toggle();
    });
    $(".ui-list-item").click(function (e) {
        e.stopPropagation();
    })
</script>
</body>
</html>