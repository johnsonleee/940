section.login{
    background-color:#fff;	
    /**background: #1D71C4;*/
    background-image: -webkit-gradient(linear, left top, left bottom, from(rgb(102, 193, 186)), to( rgb(102, 193, 186)));
    background-image: -webkit-linear-gradient( rgb(102, 193, 186), rgb(166, 224, 220));
    background-image:    -moz-linear-gradient(rgb(102, 193, 186), rgb(166, 224, 220));
    background-image:     -ms-linear-gradient( rgb(102, 193, 186), rgb(166, 224, 220));
    background-image:      -o-linear-gradient(rgb(102, 193, 186), rgb(166, 224, 220));
    background-image:         linear-gradient( rgb(102, 193, 186), rgb(102, 193, 186));
}
.swipe-login{
    background :#040700;
    margin-bottom: 0px;
}
.login .swipe-list {
    height: 170px;
    margin: 0 auto;
    overflow: hidden;
    position: relative;
}
.swipe-wrap .swipe-ul {
    padding: 0;
    margin: 0;
    position: absolute;
}
.login .swipe-ul li {
    margin: 0 8px;
    width: 300px;
    height: 170px;
    float: left;
}

.star {
	color: red;
}

.forgetpwd-content {
	padding: 10px 15px 0px 15px;
}
.btnreg {
	background-image: -webkit-gradient(linear, center bottom, center top, color-stop(0%,
		rgb(33, 125, 119)), color-stop(100%, rgb(33, 125, 119)) );
	background-image: -webkit-linear-gradient(bottom, rgb(33, 125, 119) 0%, rgb(33, 125, 119) 100%);
	background-image: -moz-linear-gradient(bottom, rgb(33, 125, 119) 0%, rgb(33, 125, 119) 100%);
	background-image: -ms-linear-gradient(bottom, rgb(33, 125, 119) 0%, rgb(33, 125, 119) 100%);
	background-image: -o-linear-gradient(bottom, rgb(33, 125, 119) 0%, rgb(33, 125, 119) 100%);
	background-image: linear-gradient(to top, rgb(33, 125, 119) 0%, rgb(33, 125, 119) 100%);
}

