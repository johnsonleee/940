html head+body .ui-btn.ui-btn-y,html head+body .ui-btn.ui-btn-y:hover {
	color:#fff;
	text-shadow:none;
	-webkit-border-radius: 10px;
	-moz-border-radius: 10px;
	border-radius: 10px;
	border: 1px solid #66C1BB; /*修改border颜色*/
	/*text-shadow: 1px 1px 0px #5E5E5E;*/
	-webkit-box-shadow: inset 1px 1px 0px 0px #66C1BB; /*修改立体boder颜色*/
	-moz-box-shadow: inset 1px 1px 0px 0px #66C1BB; /*修改立体boder颜色*/
	text-align: center;
	height: 34px;
	line-height: 34px;
	padding: 1px 0;
	background-image: -webkit-gradient(linear, center bottom, center top, color-stop(0%,
		#66C1BB), color-stop(100%, #66C1BB) );
	background-image: -webkit-linear-gradient(bottom, #66C1BB 0%, #66C1BB 100%);
	background-image: -moz-linear-gradient(bottom, #66C1BB 0%, #66C1BB 100%);
	background-image: -ms-linear-gradient(bottom, #66C1BB 0%, #66C1BB 100%);
	background-image: -o-linear-gradient(bottom, #66C1BB 0%, #66C1BB 100%);
	background-image: linear-gradient(to top, #66C1BB 0%, #66C1BB 100%);
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr="#66C1BB",
		endColorstr="#66C1BB" );
}


