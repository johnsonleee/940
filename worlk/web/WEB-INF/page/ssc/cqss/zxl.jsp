    	cwb(this);
    });
	$("#orderBtn").on("click",function(e) {
        obc();
    });
    $("#submitBtn").on("click",function(){
    	sbc();
    });
    $("#cancelBtn").on("click",function(){
    	$.mobile.changePage("#rateInfoPage", "slideup");
